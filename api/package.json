{
  "name": "streaming-backend",
  "version": "1.0.0",
  "description": "Backend API para plataforma de streaming tipo Netflix",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "node src/seeds/index.js",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed",
    "prisma:studio": "npx prisma studio",
    "test": "jest"
  },
  "keywords": ["streaming", "netflix", "api", "movies", "series"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3"
  },
  "prisma": {
    "seed": "node src/seeds/index.js"
  }
}
